      "classnames": "^2.3.1",
      "cross-env": "^7.0.3",
      "mobx": "^5.15.7",
      "mobx-react": "^6.3.1",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-router-class-tools": "^0.1.2",
      "react-router-dom": "^6.3.0"
    },
    "devDependencies": {
      "@parcel/config-default": "2.6.0",
      "@parcel/packager-raw-url": "2.6.0",
      "@parcel/transformer-less": "2.6.0",
      "@parcel/transformer-raw": "2.6.0",
      "@parcel/transformer-svg-react": "2.6.0",
      "@parcel/transformer-typescript-tsc": "2.6.0",
      "@parcel/transformer-webmanifest": "2.6.0",
      "@types/react": "^17.0.48",
      "@types/react-dom": "^17.0.17",
      "lint-staged": "^13.0.3",
      "parcel": "2.6.0",
      "postcss": "^8.4.14",
      "postcss-modules": "^4.3.1",
      "prettier": "^2.7.1",
      "process": "^0.11.10",
      "typescript": "~4.7.4",
      "workbox-cli": "^6.5.4"
    },
    "prettier": {
      "singleQuote": true,
      "trailingComma": "none",
      "arrowParens": "avoid",
      "tabWidth": 2
    },
    "lint-staged": {
      "*.{html,md,less,json,yml,js,ts,tsx}": "prettier --write"
    },
    "browserslist": "and_chr 69, safari 13, ios_saf 13",
    "scripts": {
      "test": "lint-staged",
      "start": "rm -rf dist/ .parcel-cache/  &&  parcel src/index.html",
      "pack": "parcel build src/index.html --public-url .",
      "build": "rm -rf dist/  &&  npm run pack  &&  workbox generateSW",
      "packProd": "cross-env NODE_ENV=production parcel build src/index.html --public-url .",
      "buildProd": "rm -rf dist/  &&  npm run packProd  &&  workbox generateSW"
    },
    "resolutions": {
      "@types/react": "17.0.2",
      "@types/react-dom": "17.0.2"
    }
  }
  